#import "build/symbols"

10 ' DS3231 Exemple
55 HIMEM#7FFF:CLOAD"" ' Load module
60 PRINT "I2C DS3231 set and read"
70 PRINT "Plug I2C module and press any key."
80 GET A$
100 POKE DS3231sec,#0
110 POKE DS3231min,#16
120 POKE DS3231hour,#13
130 POKE DS3231wd,#3
140 POKE DS3231day,#23
150 POKE DS3231month,#4
160 POKE DS3231year,#24
170 CALL DS3231write
180 CALL DS3231read
190 SEC=PEEK(DS3231sec)
200 MIN=PEEK(DS3231min)
210 HOUR=PEEK(DS3231hour)
220 WD=PEEK(DS3231wd)
230 DAY=PEEK(DS3231day)
240 MO=PEEK(DS3231month)
250 YEAR=PEEK(DS3231year)
260 PRINT HEX$(HOUR),HEX$(MIN),HEX$(SEC)
270 PRINT HEX$(DAY),HEX$(MO),HEX$(YEAR)
280 WAIT 100
290 GOTO 180

